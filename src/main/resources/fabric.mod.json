{
  "schemaVersion": 1,
  "id": "ffaf",
  "version": "${version}",

  "name": "Fantastic Flowers And Farming",
  "description": "Makes the Minecraft world more colorful by adding more flowers, plants and foods.",
  "authors": [
    "EmilSG",
    "Twailla",
    "E7Smy"
  ],
  "contact": {
    "homepage": "https://github.com/EmilsGithub/Fantastic-Flowers",
    "sources": "https://github.com/EmilsGithub/Fantastic-Flowers"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.emilsg.ffaf.FantasticFlowersAndFarming"
    ],
    "client":[
      "net.emilsg.ffaf.FantasticFlowersAndFarmingClient"
    ],
    "fabric-datagen":[
      "net.emilsg.ffaf.datagen.ModDataGeneration"
    ]
  },
  "mixins": [
    "ffaf.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
