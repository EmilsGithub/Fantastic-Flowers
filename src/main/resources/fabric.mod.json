{
  "schemaVersion": 1,
  "id": "fantasticflowers",
  "version": "${version}",

  "name": "Fantastic Flowers",
  "description": "Makes the Minecraft world more colorful by adding more flowers and plants.",
  "authors": [
    "EmilSG"
  ],
  "contact": {
    "homepage": "https://github.com/EmilsGithub/Fantastic-Flowers",
    "sources": "https://github.com/EmilsGithub/Fantastic-Flowers"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.emilsg.fantasticflowers.FantasticFlowers"
    ],
    "client":[
      "net.emilsg.fantasticflowers.FantasticFlowersClient"
    ]
  },
  "mixins": [
    "fantasticflowers.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
